{
    "projectId": "docify",
    "projectName": "Docify",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "sites": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true,
                "oauth": {
                    "google": true,
                    "github": true
                }
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "functions": [
        {
            "$id": "llm-analyzer",
            "name": "LLM Analyzer",
            "runtime": "node-18",
            "path": "functions/llm-analyzer",
            "entrypoint": "src/main.js",
            "commands": {
                "build": "npm install"
            },
            "scopes": ["documents.read", "documents.write", "databases.read", "databases.write"],
            "vars": {
                "DATABASE_ID": "docify_db",
                "DOCUMENTS_COLLECTION_ID": "documents_table",
                "ANALYSIS_COLLECTION_ID": "analysis_results"
            },
            "events": ["databases.docify_db.collections.analysis_results.documents.create"],
            "schedule": "",
            "timeout": 600
        },
        {
            "$id": "document-scraper-python",
            "name": "Document Scraper Python",
            "runtime": "python-3.12",
            "path": "functions/document-scraper-python",
            "entrypoint": "src/main.py",
            "commands": {
                "build": "pip install -r requirements.txt"
            },
            "scopes": ["documents.read", "documents.write", "databases.read", "databases.write"],
            "vars": {
                "DATABASE_ID": "docify_db",
                "DOCUMENTS_COLLECTION_ID": "documents_table",
                "ANALYSIS_COLLECTION_ID": "analysis_results"
            },
            "events": [],
            "schedule": "",
            "timeout": 300
        }
    ],
    "sites": [
        {
            "$id": "docify-website",
            "name": "Docify-website",
            "enabled": true,
            "logging": true,
            "framework": "sveltekit",
            "timeout": 30,
            "installCommand": "npm install",
            "buildCommand": "npm run build",
            "outputDirectory": "./build",
            "specification": "s-1vcpu-512mb",
            "buildRuntime": "node-22",
            "adapter": "ssr",
            "fallbackFile": "",
            "path": "sites/Docify-website"
        }
    ]
}